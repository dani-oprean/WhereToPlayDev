@{ ViewBag.Title = "Contacteaza-ne"; }

<p>Pentru orice problema va rugam nu ezitati sa ne contactati!</p>

<form method="get" id="formulmeu">
    <div class="form_settings">
        <p id="namep"><span>Nume</span><input class="contact" type="text" name="your_name" value="" /></p>
        <p id="emailp"><span>Adresa de Email</span><input class="contact" type="text" name="your_email" value="" /></p>
        <p><span>Mesaj</span><textarea class="contact textarea" rows="8" cols="50" name="your_enquiry"></textarea></p>
        <p name="wordCount"><span>Cuvinte</span><input class="contact" type="text" name="your_wordCount" /></p>
        <p style="padding-top: 15px"><span>&nbsp;</span><input class="submit" type="submit" name="contact_submitted" value="Trimite"  id="trimitecontact"/></p>
    </div>
</form>
<script src="~/Scripts/jquery-2.2.0.js"></script>
<script src="~/Scripts/ValidateContactForm.js"></script>